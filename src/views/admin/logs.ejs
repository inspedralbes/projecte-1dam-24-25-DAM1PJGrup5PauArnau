<%- include('../partials/header', { title: "Panell de Logs" }) %>

<h1>ðŸ“Š Logs d'accÃ©s</h1>

<table class="table table-bordered" table-striped align="middle">
  <thead class="table-dark text-center">
    <tr>
      <th>Usuari</th>
      <th>URL</th>
      <th>Navegador</th>
      <th>Data</th>
    </tr>
  </thead>
  <tbody class="text-center">
    <% logs.forEach(log => { %>
      <tr>
        <td><%= log.user %></td>
        <td><%= log.url %></td>
        <td><%= log.userAgent %></td>
        <td><%= new Date(log.timestamp).toLocaleString() %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

